[core]
	excludesfile = ~/.gitignore
	editor = vim
	autocrlf = input
[user]
	name = Jan Martinec
[pull]
	ff = only
[alias]
	co = checkout
	cp = cherry-pick
	p = pull
	squash = merge --squash
	st = status
	df = diff
	b = branch
	r = reset
	rv = checkout --
	ours = checkout --ours --
	theirs = checkout --theirs --
	sl = stash list
	sa = stash apply
	ss = stash save
	alias = config --get-regexp 'alias.*'
	logol = log --pretty=format:"%h\\ %s\\ [%cn]"
	search = "grep -Iin"
	vup = !vagrant up
	vsu = !vagrant suspend
	vss = !vagrant ssh
	vde = !vagrant destroy
	vpr = !vagrant provision
	git = !exec git
[help]
	autocorrect = 30
